{
  "name": "blog-api-the-odin-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x bun --env-file=.env.development.local src/server/main.ts",
    "start": "NODE_ENV=production bun --env-file=.env.production.local src/server/main.ts",
    "build": "vite build",
    "prisma:dev": "bunx prisma migrate dev",
    "prisma:dev:migrate": "bun --env-file=.env.development.local prisma:dev --name",
    "prisma:generate": "bunx prisma generate",
    "prisma:generate:watch": "bunx prisma generate --watch"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.3.0",
    "typescript": "^5.3.2",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@vitejs/plugin-react": "^4.2.0",
    "nodemon": "^3.0.1",
    "prisma": "^6.0.1",
    "vite": "^5.0.2"
  }
}
