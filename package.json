{
  "name": "blog-api-the-odin-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x bun --env-file=.env.development.local src/server/main.ts",
    "start": "NODE_ENV=production bun --env-file=.env.production.local src/server/main.ts",
    "build": "vite build",
    "prisma:dev": "prisma migrate dev",
    "prisma:dev:migrate": "bun --env-file=.env.development.local prisma:dev --name",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "lint": "bun --bunx eslint .",
    "lint:fix": "bun:lint --fix",
    "format": "bun --bunx prettier . --write",
    "seed:dev": "bun --env-file=.env.development.local scripts/seed.ts",
    "seed:prod": "bun --env-file=.env.production.local scripts/seed.ts"
  },
  "dependencies": {
    "@hikyu/log": "npm:@jsr/hikyu__log",
    "@prisma/client": "^6.0.1",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-router": "^1.88.0",
    "@valibot/valibot": "npm:@jsr/valibot__valibot@0.42.1",
    "axios": "^1.7.9",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.3.0",
    "typescript": "^5.7.2",
    "vite-express": "*"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.19.0",
    "@eslint/js": "^9.16.0",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@tanstack/eslint-plugin-router": "^1.87.3",
    "@tanstack/react-query-devtools": "^5.62.7",
    "@tanstack/router-devtools": "^1.88.0",
    "@tanstack/router-plugin": "^1.87.13",
    "@types/bun": "^1.1.14",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.9.3",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.20",
    "eslint": "^9.16.0",
    "eslint-config-love": "^110.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.0.1",
    "tailwindcss": "^3.4.16",
    "typescript-eslint": "^8.18.0",
    "vite": "^5.0.2"
  }
}
