{
  "name": "blog-api-the-odin-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x bun --env-file=.env.development.local src/server/main.ts",
    "start": "NODE_ENV=production bun --env-file=.env.production.local src/server/main.ts",
    "build": "vite build",
    "prisma:dev": "bunx prisma migrate dev",
    "prisma:dev:migrate": "bun --env-file=.env.development.local prisma:dev --name",
    "prisma:generate": "bunx prisma generate",
    "prisma:generate:watch": "bunx prisma generate --watch"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.3.0",
    "typescript": "^5.7.2",
    "vite-express": "*"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.19.0",
    "@eslint/js": "^9.16.0",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@tanstack/eslint-plugin-router": "^1.87.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.20",
    "eslint": "^9.16.0",
    "eslint-config-love": "^110.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.0.1",
    "tailwindcss": "^3.4.16",
    "typescript-eslint": "^8.18.0",
    "vite": "^5.0.2"
  }
}
